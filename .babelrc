{
  "presets": ["es2015", "react"],
  "plugins": [
    "transform-runtime",
    "transform-class-properties",
    "react-hot-loader/babel",
    "babel-plugin-transform-object-rest-spread"
  ],
  "env": {
    "dev": {
      "plugins": [
        "transform-react-jsx-source",
        "transform-react-jsx-self"
      ]
    }
  }
}
